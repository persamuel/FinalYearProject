int strcpy(char[] src, char[] dest);

int main(int argc, char[][] argv) {
    int tmp;
    char[500] buf;

    tmp = strcpy(argv[1], buf);
    print(buf);

    return 0;
}

int strcpy(char[] src, char[] dest) {
    int i;
    i = 0;

    while (src[i] != 0) {
        dest[i] = src[i];
        i = i + 1;
    }

    return 1;
}
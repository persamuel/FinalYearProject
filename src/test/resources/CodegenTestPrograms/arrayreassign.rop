/* The output on stdout should be the first argument passed ++ Hello World!
 */
int main(int argc, char[][] argv) {
    char[] s;

    s = argv[0];

    print(s);

    s = new char[12];

    s[0] = 'H';
    s[1] = 'e';
    s[2] = 'l';
    s[3] = 'l';
    s[4] = 'o';
    s[5] = ' ';
    s[6] = 'W';
    s[7] = 'o';
    s[8] = 'r';
    s[9] = 'l';
    s[10] = 'd';
    s[11] = '!';
    s[12] = '\0';

    print(s)

    free s;

    return 0;
}

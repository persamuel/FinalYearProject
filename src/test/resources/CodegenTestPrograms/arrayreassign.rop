/* The output on stdout should be the first argument passed ++ World!
 */
int main(int argc, char[][] argv) {
    char[] s;

    s = argv[1];

    print(s);

    s = new char[7];

    s[0] = 'W';
    s[1] = 'o';
    s[2] = 'r';
    s[3] = 'l';
    s[4] = 'd';
    s[5] = '!';
    s[6] = '\0';

    print(s);

    free s;

    return 0;
}

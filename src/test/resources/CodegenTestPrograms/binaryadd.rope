int binaryadd(char[] s1, char[] s2);
int stringlength(char[] s);
int decconvert(char[] s);

/* The output on stdout should be the decimal equivalent of the two binary numbers passed as arguments.
 */
int main(int argc, char[][] argv) {
    print(binaryadd(argv[1], argv[2]));
    return 0;
}

int binaryadd(char[] s1, char[] s2) {
    return decconvert(s1) + decconvert(s2);
}

int decconvert(char[] s) {
    int n;
    int b;
    int i;
    int pow;

    n = stringlength(s);
    pow = 1;
    i = n - 1;
    b = 0;
    while (i >= 0) {
        if (s[i] == '1') {
            b = b + pow;
        } else { // Do nothing }

        pow = pow * 2;
        i = i - 1;
    }

    return b;
}

int stringlength(char[] s) {
    int i;

    i = 0;
    while (s[i] != '\0') {
        i = i + 1;
    }

    return i;
}
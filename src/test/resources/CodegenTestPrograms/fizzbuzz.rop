int fizzbuzz(int n);
int quotient(int numerator, int denominator);
int remainder(int numerator, int denominator);

int main(int argc, char[][] argv) {
    int tmp;

    tmp = fizzbuzz(30);

    return 0;
}

int fizzbuzz(int n) {
    int x;

    x = 0;
    while(x <= n) {
        print(x);
        print(':');

        if (remainder(x,3) == 0) {
            print('f');
        }
        else {
            x = x;
        }

        if (remainder(x,5) == 0) {
            print('b');
        }
        else {
           x = x;
        }

        print(' ');

        x = x + 1;
    }

    return 0;
}

int remainder(int numerator, int denominator) {
    int x;

    x = quotient(numerator, denominator);

    return numerator - (x * denominator);
}

int quotient(int numerator, int denominator) {
    int fraction;
    int remainder;

    fraction = 0;
    remainder = numerator;

    while (remainder >= denominator) {
        remainder = remainder - denominator;
        fraction  = fraction + 1;
    }

    return fraction;
}
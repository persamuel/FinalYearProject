int product(int[] A, int n);

/* The output on stdout should be 560,3360
 */
int main(int argc, char[][] argv) {
    int[5] stack;
    int[] heap;
    int n;

    n = 5;
    stack[0] = 5;
    stack[1] = 2;
    stack[2] = 7;
    stack[3] = 8;
    stack[4] = 1;

    print(product(stack, n));

    print(',');

    n = 6;
    heap = new int[6];
    heap[0] = 5;
    heap[1] = 2;
    heap[2] = 7;
    heap[3] = 8;
    heap[4] = 1;
    heap[5] = 6;

    print(product(heap, n));

    free heap;

    return 0;
}

int product(int[] A, int n) {
    int i;
    int product;

    product = 1;
    i = 0;
    while (i < n) {
        product = product * A[i];
        i = i + 1;
    }

    return product;
}
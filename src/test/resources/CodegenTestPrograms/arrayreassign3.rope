int printarray(int[] A, int n);

/* The output on stdout should be 1,2,3,4,5
 */
int main(int argc, char[][] argv) {
    int[5] arr1;
    int[] arr2;
    int tmp;

    arr2 = arr1;

    arr2[0] = 1;
    arr2[1] = 2;
    arr2[2] = 3;
    arr2[3] = 4;
    arr2[4] = 5;

    tmp = printarray(arr2, 5);


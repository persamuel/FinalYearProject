int stringlength(char[] s);
char[] reverse(char[] s);

/* The output on std should be the reverse of whatever was passed as arguments.
 */
int main(int argc, char[][] argv) {
    int i;
    char[] tmp;

    i = 0;
    while (i < argc) {
        tmp = reverse(argv[i]);
        print(tmp);
        free tmp;
        i = i + 1;
    }

    return 0;
}

int stringlength(char[] s) {
    int i;

    i = 0;
    while (s[i] != '\0') {
        i = i + 1;
    }

    return i;
}

char[] reverse(char[] s) {
    char[] tmp;
    int n;
    int i;
    int j;

    n = stringlength(s);
    tmp = new char[n + 1];

    j = 0;
    i = n;
    while (i >= 0) {
        tmp[j] = s[i];
        j = j + 1;
        i = i - 1;
    }

    return tmp;
}
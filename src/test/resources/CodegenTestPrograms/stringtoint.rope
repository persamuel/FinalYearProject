int stringtoint(char[] s);
int stringlength(char[] s);

/* The output on stdout should be the integer passed in
 */
int main(int argc, char[][] argv) {
    print(stringtoint(argv[1]));

    return 0;
}

int stringtoint(char[] s) {
    int x;
    int len;
    int i;
    int multi;

    x = 0;
    len = stringlength(s);
    i = len - 1;
    multi = 1;

    while (i >= 0) {
        x = x + (s[i] - '0') * multi;
        multi = multi * 10;
        i = i - 1;
    }

    return x;
}

int stringlength(char[] s) {
    int i;

    i = 0;
    while (s[i] != '\0') {
        i = i + 1;
    }

    return i;
}
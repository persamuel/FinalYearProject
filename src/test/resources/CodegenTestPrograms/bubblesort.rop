int bubblesort(int[] A, int n);
int printarray(int[] A, int n);

/* The output on stdout should be 120,240,360,480,600
 */
int main(int argc, char[][] argv) {
    int[3] stack;
    int[] heap;
    int tmp;

    stack[0] = 360;
    stack[1] = 240;
    stack[2] = 120;

    tmp = bubblesort(stack, 3);
    tmp = printarray(stack, 3);
	print(',');

    heap = new int[2];

    heap[0] = 600;
    heap[1] = 480;

    tmp = bubblesort(heap, 3);
    tmp = printarray(heap, 2);

    free heap;

    return 0;
}

int printarray(int[] A, int n) {
    int i;

    i = 0;
    while (i < n) {
        print(A[i]);
        i = i + 1;
        if (i < n)
            print(',');
        else
            i = i;
    }

    return 1;
}

int bubblesort(int[] A, int n) {
    int i;
    int j;
    int tmp;

    i = 0;
    while (i < n - 1) {
        j = 0;
        while (j < (n - i - 1)) {
            if (A[j] > A[j + 1]) {
                tmp = A[j];
                A[j] = A[j + 1];
                A[j + 1] = tmp;
            } else {
                j = j; // Do nothing
            }
            j = j + 1;
        }
        i = i + 1;
    }

    return 1;
}
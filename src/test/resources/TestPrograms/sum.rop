int sum(int[] A, int n);

/* The output on stdout should be 15,21
 */
int main(int argc, char[][] argv) {
    int[5] stack;
    int[] heap;
    int n;

    n = 5;
    stack[0] = 1;
    stack[1] = 2;
    stack[2] = 3;
    stack[3] = 4;
    stack[4] = 5;

    print(sum(stack, n));

    print(',');

    n = 6;
    heap = new int[6];
    heap[0] = 1;
    heap[1] = 2;
    heap[2] = 3;
    heap[3] = 4;
    heap[4] = 5;
    heap[5] = 6;

    print(sum(heap, n));

    free heap;

    return 0;
}

int sum(int[] A, int n) {
    int i;
    int sum;

    sum = 0;
    i = 0;
    while (i < n) {
        sum = sum + A[i];
        i = i + 1;
    }

    return sum;
}
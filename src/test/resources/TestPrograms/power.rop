int power(int base, int exponent);

/* The output on stdout should be 9
 */
int main(int argc, char[][] argv) {
    print(power(3, 2));
    return 0;
}

int power(int base, int exponent) {
    int x;

    if (exponent == 0)
        x = 1;
    else
        x = base * power(base, exponent - 1);

    return x;
}